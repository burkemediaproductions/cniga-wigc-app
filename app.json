{
  "expo": {
    "name": "CNIGA WIGC",
    "slug": "cniga-wigc-native",
    "scheme": "cniga",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "bundleIdentifier": "com.wigc.ios",
      "supportsTablet": true
    },

    "android": {
      "package": "com.wigc.android",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow WIGC to access your photos so you can upload a profile picture.",
          "cameraPermission": "Allow WIGC to use your camera so you can take a profile picture."
        }
      ]
    ],
